{
    "swagger": "2.0",
    "info": {
        "description": "API for Polyamanita server functions",
        "title": "Polyamanita API",
        "contact": {},
        "version": "1.0"
    },
    "host": "\u003csome url\u003e",
    "paths": {
        "/auths": {
            "post": {
                "description": "Sends an email to the address passed in with a verification code to verify their email",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Auth"
                ],
                "summary": "Send a Verification Code",
                "parameters": [
                    {
                        "description": "Email address to send code to",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/routes.AuthEmail.AuthEmailInputStruct"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Expiry time of code",
                        "schema": {
                            "$ref": "#/definitions/routes.AuthEmail.AuthEmailOutputStruct"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error"
                    }
                }
            }
        }
    },
    "definitions": {
        "routes.AuthEmail.AuthEmailInputStruct": {
            "type": "object",
            "properties": {
                "email": {
                    "type": "string"
                }
            }
        },
        "routes.AuthEmail.AuthEmailOutputStruct": {
            "type": "object",
            "properties": {
                "codeExpiry": {
                    "type": "string"
                }
            }
        }
    }
}